{
  "name": "mingyue-design",
  "version": "0.1.1",
  "private": false,
  "description": "A components library for React",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "mingyue",
  "homepage": "https://github.com/mingyue-design/mingyue-design",
  "repository": {
    "type": "git",
    "url": "https://github.com/mingyue-design/mingyue-design"
  },
  "keywords": [
    "react",
    "components",
    "library",
    "UI"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "axios": "^1.7.8",
    "better-scroll": "^2.5.1",
    "classnames": "^2.5.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.13"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:ts && npm run build:css",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:css": "sass src/styles/index.scss dist/index.css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "npm run lint && npm run test:nowatch && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-onboarding": "^8.4.6",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.4.6",
    "@storybook/cli": "^8.4.6",
    "@storybook/preset-create-react-app": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-webpack5": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.4",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.119",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-transition-group": "^4.4.11",
    "cross-env": "^7.0.3",
    "eslint-plugin-storybook": "^0.11.1",
    "husky": "^9.1.7",
    "jest": "^27.5.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "storybook": "^8.4.6",
    "typescript": "^4.9.5",
    "webpack": "^5.96.1"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  }
}
